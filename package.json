{
  "name": "@eliyya/type-routes",
  "version": "2.4.0",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "import": "./lib/index.mjs"
    },
    "./next": {
      "import": "./lib/next.mjs"
    }
  },
  "bin": {
    "type-routes": "lib/cli.mjs"
  },
  "scripts": {
    "build": "tsc && esbuild src/*.mts --minify --platform=node --outdir=lib --out-extension:.js=.mjs",
    "postinstall": "node lib/cli.mjs || 0"
  },
  "keywords": [
    "typescript",
    "next",
    "nextjs",
    "routes"
  ],
  "author": "eliyya",
  "license": "MIT",
  "description": "Type your Nextjs routes",
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@types/node": "22.13.9",
    "esbuild": "0.25.0",
    "eslint": "9.21.0",
    "globals": "16.0.0",
    "next": "15.2.4",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0"
  },
  "peerDependencies": {
    "next": ">=14"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": false
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20.11"
  }
}